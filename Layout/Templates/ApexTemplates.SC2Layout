                    <Key type="Curve" time="0.0" value="55"/>
                    <Key type="Curve" time="0.15" value="165"/>
                </Controller>
                <Controller type="Color" end="Pause" frame="$this/Button/NormalImage">
                    <Key type="Curve" time="0.0" value="175,175,175"/>   
                    <Key type="Curve" time="0.15" value="255,255,255"/>
                </Controller>
            </Animation>
            
            <Animation name="HoverOut">
                <Event event="OnMouseExit" action="Reset,Play" frame="$this"/>
                <Controller type="Fade" end="Pause" frame="$this/Tooltip">
                    <Key type="Curve" time="0.0" value="165"/>
                    <Key type="Curve" time="0.15" value="55"/>
                </Controller>
                <Controller type="Color" end="Pause" frame="$this/Button/NormalImage">
                    <Key type="Curve" time="0.0" value="255,255,255"/>   
