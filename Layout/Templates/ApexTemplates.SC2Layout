                <Anchor side="Right" relative="$parent/Button" pos="Max" offset="0"/>
                <Width val="60"/>
                <Height val="20"/>
                <Style val="apx_regular_center_mid"/>
                <Text val="127"/>
                <Unclipped val="True"/>
                <Alpha val="55"/>
            </Frame>
    
            <Frame type="Button" name="Button">
                <Width val="12"/>
                <Height val="12"/>
                <AcceptsMouse val="False"/>
                <Frame type="Image" name="NormalImage">
                    <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Texture val="Assets/Textures/white32.dds"/>
                    <Color val="175,175,175"/>
                    <Width val="12"/>
                    <Height val="12"/>
                    <Rotation val="45"/>
                    <RotationPosition val="MiddleCenter"/>
                </Frame>
            </Frame>
        </Frame>
    </Frame>

    
    <!-- Icon Button -->
    <Frame type="Button" name="ApexIconButtonNoOutline">
        <Visible val="True"/>
        <RenderPriority val="8"/>
        <AcceptsMouse val="True"/>
        <Toggled val="False"/>

        <Animation name="Hover">
            <Event event="OnMouseEnter" action="Reset,Play" frame="$this"/>
            <Controller type="Fade" end="Pause" frame="$this/Text">
                <Key type="Curve" time="0.0" value="150"/>
                <Key type="Curve" time="0.15" value="255"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/Icon">
                <Key type="Curve" time="0.0" value="150"/>
                <Key type="Curve" time="0.15" value="255"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/TooltipFrame">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.15" value="245"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/Tooltip">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.15" value="165"/>
            </Controller>
        </Animation>
        
        <Animation name="HoverOff">
            <Event event="OnMouseExit" action="Reset,Play" frame="$this"/>
            <Controller type="Fade" end="Pause" frame="$this/Text">
                <Key type="Curve" time="0.0" value="255"/>
                <Key type="Curve" time="0.15" value="150"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/Icon">
                <Key type="Curve" time="0.0" value="255"/>
                <Key type="Curve" time="0.15" value="150"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/TooltipFrame">
                <Key type="Curve" time="0.0" value="245"/>
                <Key type="Curve" time="0.15" value="0"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/Tooltip">
                <Key type="Curve" time="0.0" value="165"/>
                <Key type="Curve" time="0.15" value="0"/>
            </Controller>
        </Animation>

        <Animation name="Disable">
            <Event event="OnDisabled" action="Reset,Play" frame="$this"/>
            <Controller type="Fade" end="Pause" frame="$this/TooltipFrame">
                <Key type="Curve" time="0.0" value="245"/>
                <Key type="Curve" time="0.15" value="0"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/Tooltip">
                <Key type="Curve" time="0.0" value="165"/>
                <Key type="Curve" time="0.15" value="0"/>
            </Controller>
        </Animation>

        <Animation name="Enabled">
            <Event event="OnEnabled" action="Reset,Play" frame="$this"/>
            <Controller type="Fade" end="Pause" frame="$this/TooltipFrame">
                <Key type="Curve" time="0.0" value="0"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/Tooltip">
                <Key type="Curve" time="0.0" value="0"/>
            </Controller>
        </Animation>


        <Frame type="Label" name="Text">
            <Anchor relative="$parent"/>
            <Style val="apx_regular_center_mid"/>
            <Text val=""/>
            <Alpha val="150"/>
        </Frame>

        <Frame type="Image" name="Icon">
            <Texture val="Assets/Textures/blank.dds"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="7"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="7"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-7"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-7"/>
            <Alpha val="150"/>
            <RenderPriority val="9"/>
        </Frame>
        <!-- Tooltip -->
        <Frame type="Frame" name="TooltipFrame" template="ApexTemplates/ApexSecondaryFrameTemplate">
			<Unclipped val="True"/>
            <IgnoreParentAlpha val="True"/>
			<Alpha val="0"/>
            <Anchor side="Left" relative="$parent/Tooltip" pos="Min" offset="-5"/>
            <Anchor side="Right" relative="$parent/Tooltip" pos="Max" offset="5"/>
			<Anchor side="Bottom" relative="$parent/Tooltip" pos="Max" offset="5"/>
			<Anchor side="Top" relative="$parent/Tooltip" pos="Min" offset="-5"/>

            
			<Frame type="Image" name="Background">
                <Alpha val="255"/>
            </Frame>
		</Frame>

        <Frame type="Label" name="Tooltip">
            <IgnoreParentAlpha val="True"/>
            <Unclipped val="True"/>
            <Alpha val="0"/>
            <Style val="apx_regular_center_mid"/>
            <Text val=""/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Bottom" relative="$parent" pos="Min" offset="-5"/>
        </Frame>



    </Frame>

    <!-- Rectangular Button -->
    <Frame type="Button" name="ApexRectangularButton">
        <Visible val="True"/>
        <RenderPriority val="8"/>
        <AcceptsMouse val="True"/>
        <Toggleable val="True"/>
        <Toggled val="False"/>
        <Frame type="Image" name="Border">
            <TextureType val="Border"/>
            <Texture val="Apex/Assets/Textures/GenericFrame/simple_outline_border.dds"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Alpha val="50"/>
            <RenderPriority val="9"/>
        </Frame>
        <Frame type="Image" name="BackgroundColor">
            <TextureType val="Border"/>
            <Texture val="Apex/Assets/Textures/GenericFrame/simple_outline_background.dds"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Alpha val="25"/>
            <Color val="0,0,0"/>
            <RenderPriority val="7"/>
        </Frame>
        <Frame type="Image" name="BackgroundColorHover">
            <TextureType val="Border"/>
            <Texture val="Apex/Assets/Textures/GenericFrame/simple_outline_background.dds"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Alpha val="0"/>
            <Color val="255,255,255"/>
            <RenderPriority val="8"/>
        </Frame>
        <Frame type="Image" name="BackgroundLineHover">
            <TextureType val="Border"/>
            <Texture val="Apex/Assets/Textures/GenericFrame/simple_outline_border.dds"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-6"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-6"/>
            <Alpha val="0"/>
            <Color val="255,255,255"/>
            <RenderPriority val="8"/>
        </Frame>
        
        <Frame type="Image" name="BackgroundColorToggle">
            <TextureType val="Border"/>
            <Texture val="Apex/Assets/Textures/GenericFrame/simple_outline_background.dds"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Alpha val="0"/>
            <Color val="255,255,255"/>
            <RenderPriority val="8"/>
        </Frame>
        <Frame type="Image" name="BackgroundLineToggle">
            <TextureType val="Border"/>
            <Texture val="Apex/Assets/Textures/GenericFrame/simple_outline_border.dds"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="6"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-6"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-6"/>
            <Alpha val="0"/>
            <Color val="255,255,255"/>
            <RenderPriority val="8"/>
        </Frame>

        <Animation name="Toggle">
            <Event event="Normal" action="Reset" frame="$this"/>
            <Event event="Toggled"  action="Reset,Play" frame="$this"/>
            <Controller type="Fade" end="Pause" frame="$this/BackgroundColorToggle">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.0" value="35"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/BackgroundLineToggle">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.0" value="165"/>
            </Controller>
            <Controller type="Anchor" side="Top" relative="False" end="Pause" frame="$this/BackgroundLineToggle">
                <Key type="Curve" time="0.0" value="6"/>
                <Key type="Curve" time="0.0" value="0"/>
            </Controller>
            <Controller type="Anchor" side="Left" relative="False" end="Pause" frame="$this/BackgroundLineToggle">
                <Key type="Curve" time="0.0" value="6"/>
                <Key type="Curve" time="0.0" value="0"/>
            </Controller>
            <Controller type="Anchor" side="Right" relative="False" end="Pause" frame="$this/BackgroundLineToggle">
                <Key type="Curve" time="0.0" value="-6"/>
                <Key type="Curve" time="0.0" value="0"/>
            </Controller>
            <Controller type="Anchor" side="Bottom" relative="False" end="Pause" frame="$this/BackgroundLineToggle">
                <Key type="Curve" time="0.0" value="-6"/>
                <Key type="Curve" time="0.0" value="0"/>
            </Controller>
        </Animation>

        <Animation name="Hover">
            <Event event="Toggled" action="Reset,Play,DirectionReverse" frame="$this"/>
            <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$this"/>
            <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$this"/>
            <Controller type="Fade" end="Pause" frame="$this/BackgroundColorHover">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.2" value="35"/>
            </Controller>
            <Controller type="Fade" end="Pause" frame="$this/BackgroundLineHover">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.2" value="165"/>
            </Controller>
            <Controller type="Anchor" side="Top" relative="False" end="Pause" frame="$this/BackgroundLineHover">
                <Key type="Curve" time="0.0" value="6"/>
                <Key type="Curve" time="0.2" value="0"/>
            </Controller>
            <Controller type="Anchor" side="Left" relative="False" end="Pause" frame="$this/BackgroundLineHover">
                <Key type="Curve" time="0.0" value="6"/>
                <Key type="Curve" time="0.2" value="0"/>
            </Controller>
            <Controller type="Anchor" side="Right" relative="False" end="Pause" frame="$this/BackgroundLineHover">
                <Key type="Curve" time="0.0" value="-6"/>
                <Key type="Curve" time="0.2" value="0"/>
            </Controller>
            <Controller type="Anchor" side="Bottom" relative="False" end="Pause" frame="$this/BackgroundLineHover">
                <Key type="Curve" time="0.0" value="-6"/>
                <Key type="Curve" time="0.2" value="0"/>
            </Controller>
        </Animation>

    </Frame>

