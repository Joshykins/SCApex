                        <Key type="Curve" time="0.0" value="45"/>
                        <Key type="Curve" time="0.15" value="0"/>
                    </Controller>
                    <Controller type="Fade" end="Pause" frame="$parent/Border">
                        <Key type="Curve" time="0.0" value="85"/>
                        <Key type="Curve" time="0.15" value="50"/>
                    </Controller>
                </Animation>
        
                <Frame type="Frame" name="LabelContainerFrame">
                    <RenderPriority val="9"/>
                    <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
                    <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
                    <Frame type="Label" name="Label">
                        <Anchor relative="$parent"/>
                        <BaseHeight val="40"/>
                        <Style val="apx_regular_left_mid"/>
                    </Frame>
                </Frame>
            </Frame>
        </Frame>
    <!-- END of Edit box padding -->
    </Frame>



    <!-- Apex Slider -->
    <Frame type="Frame" name="ApexSlider">
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
