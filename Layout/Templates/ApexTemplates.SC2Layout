                <Animation name="Focus">
                    <Event event="OnFocusGained" action="Reset,Play" frame="$this"/>
                    <Controller type="Fade" end="Pause" frame="$parent/Focused">
                        <Key type="Curve" time="0.0" value="0"/>
                        <Key type="Curve" time="0.15" value="45"/>
                    </Controller>
                    <Controller type="Fade" end="Pause" frame="$parent/Border">
                        <Key type="Curve" time="0.0" value="50"/>
                        <Key type="Curve" time="0.15" value="85"/>
                    </Controller>
                </Animation>
                
                <Animation name="Unfocus">
                    <Event event="OnFocusLost" action="Reset,Play" frame="$this"/>
